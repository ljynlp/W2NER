{
  "dataset": "cadec",
  "save_path": "model.pt",
  "predict_path": "output.json",

  "dist_emb_size": 20,
  "type_emb_size": 20,
  "lstm_hid_size": 512,
  "conv_hid_size": 80,
  "bert_hid_size": 768,
  "biaffine_size": 512,
  "ffnn_hid_size": 120,
  "dilation": [1, 2, 3],

  "emb_dropout": 0.5,
  "conv_dropout": 0.5,
  "out_dropout": 0.33,

  "epochs": 10,
  "batch_size": 16,

  "learning_rate": 1e-3,
  "weight_decay": 0,
  "clip_grad_norm": 5.0,

  "bert_name": "dmis-lab/biobert-base-cased-v1.2",
  "bert_learning_rate": 5e-6,
  "warm_factor": 0,

  "use_bert_last_4_layers": true,
  "seed": 123

}